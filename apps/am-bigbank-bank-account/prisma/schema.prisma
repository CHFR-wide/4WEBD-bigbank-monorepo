datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_MS_BANK")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x", "linux-musl-arm64-openssl-3.0.x"]
  output   = "../node_modules/prisma-client"
}

model BankAccount {
  id      Int     @id @default(autoincrement())
  label   String
  userId  Int
  balance Decimal

  @@map("bank_account")
}